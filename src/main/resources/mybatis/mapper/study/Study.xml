<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.taobao.mapper.study.StudyMapper">

    <select id="findStudyInfoList" resultType="cn.taobao.entity.study.Study">
           select * from study
    </select>

    <select id="findStudyInfo"  resultType="cn.taobao.entity.study.Study">
        select * from study where name=#{name}
    </select>
</mapper>